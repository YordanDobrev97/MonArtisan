@using MonArtisan.Common
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@this.ViewData["Title"] - Magic Service</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/css/all.min.css" />
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-F3w7mX95PdgyTmZZMECAngseQB83DfGTowi0iMjiWaeVhAn4FJkqJByhZMI3AhiU" crossorigin="anonymous">

    <style>
        .header-search {
            position: relative;
            right: 34px;
            top: 5px;
        }
    </style>
</head>
<body>
    <header>
        <!-- Navbar -->
        <nav class="navbar navbar-expand-lg navbar-expand-sm navbar-expand-md navbar-light bg-light">
            <!-- Container wrapper -->
            <div class="container-fluid" style="display: flex; align-items: baseline;">
                <!-- Navbar brand -->
                <a class="navbar-brand me-2" href="/">
                    Magic Service
                </a>

                <!-- Toggle button -->
                <button class="navbar-toggler"
                        type="button"
                        data-mdb-toggle="collapse"
                        data-mdb-target="#navbarButtonsExample"
                        aria-controls="navbarButtonsExample"
                        aria-expanded="false"
                        aria-label="Toggle navigation">
                    <i class="fas fa-bars"></i>
                </button>

                <!-- Collapsible wrapper -->
                <div class="collapse navbar-collapse" id="navbarButtonsExample" style="justify-content: flex-end;">
                    <!-- Left links -->
                    <ul class="nav-items">
                        <li class="item">
                            <a asp-area="Identity" asp-page="/Account/Login">Connexion</a>
                        </li>
                        <li class="registration item nav-item">
                            <a class="dropbtn">S’inscrire</a>
                            <i class="fas fa-sort-down" style="position: relative; bottom: 3px; font-size: 20px;"></i>
                            <div class="dropdown-content">
                                <div>
                                    <a asp-area="Identity" asp-page="/Account/CraftsmanRegister">Créer votre compte artisan</a>
                                </div>
                                <div>
                                    <a asp-area="Identity" asp-page="/Account/ClientRegister">Créer votre compte client</a>
                                </div>
                            </div>
                        </li>
                        <li class="nav-item">
                            <a href="#">À propos</a>
                            <i class="fa fa-search icon-search"></i>
                        </li>
                    </ul>
                </div>
                <!-- Collapsible wrapper -->
            </div>
            <!-- Container wrapper -->
        </nav>
        <!-- Navbar -->

        <article class="header-article">
            <input class="header-input"
                   type="text"
                   placeholder="SAISISSEZ VOTRE PROJET..." />
            <a href="/Identity/Account/Login">
                <i class="fa fa-search header-search"></i>
            </a>
        </article>
    </header>

    <div class="container">
        <partial name="_CookieConsentPartial" />
        <div role="main" class="pb-3">
            @this.RenderBody()
        </div>
    </div>

    <footer>
        <section class="top">
            <h4>Blahblah</h4>
            <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Auctor in viverra sit dolor duis donec arcu pellentesque leo. At placerat blandit tortor, sapien. Bibendum massa in arcu ut non tellus non, sapien nulla. Euismod enim sagittis faucibus rutrum ut dignissim maecenas.</p>
            <img alt="" src="data:image/png;base64," />
        </section>

        <section class="down">
            <article>
                <div class="link-items">
                    <a href="#">A propos</a>
                </div>
                <div class="link-items">
                    <a href="#">Nous rejoindre</a>
                </div>
                <div class="link-items">
                    <a href="#">FAQ</a>
                </div>
            </article>

            <article class="middle">
                <div class="link-items">
                    <a href="#">Réseaux sociaux:</a>
                </div>

                <div class="social-media">
                    <div class="link-items">
                        <a href="#">
                            <img alt="google-logo" class="google" src="https://media.wired.com/photos/5926ffe47034dc5f91bed4e8/master/pass/google-logo.jpg" />
                        </a>
                    </div>
                    <div class="link-items">
                        <a href="#">
                            <img alt="facebok-logo" class="facebook" src="https://png.pngitem.com/pimgs/s/18-185851_facebook-logo-vector-logovectornet-facebook-logo-png-2019.png" />
                        </a>
                    </div>
                </div>
            </article>
            <article>
                <div class="link-items">
                    <a href="#">Connectez-vous</a>
                </div>
                <div class="link-items">
                    <a href="#">Créer votre compte client</a>
                </div>
                <div class="link-items">
                    <a href="#">Rejoignez notre réseau profesionnelle</a>
                </div>
            </article>

            <article class="special">
                <div class="special-link-item">
                    <a href="#">Mentions légales</a>
                </div>
                <div class="special-link-item">
                    <a>CGV</a>
                </div>
                <div class="special-link-item">
                    <a href="#">Politique de confidentialité</a>
                </div>
            </article>
        </section>

    </footer>

    <script>
        function responsive() {
            const currentItem = document.querySelector(".icon");

            if (!currentItem.className.includes("active")) {
                currentItem.classList.add("active");
                const navItems = document.querySelector(".nav-items");
                const liItems = document.querySelectorAll(".item");
                const nav = document.getElementsByTagName("nav")[0];
                const navBrand = document.querySelector(".navbar-brand");
                const hamburger = document.querySelector(".hamburger");
                const icon = document.querySelector(".straight-line");

                icon.style.top = "30.7em";

                hamburger.style.display = "block";
                hamburger.style.position = "relative";
                hamburger.style.left = "200px";
                hamburger.style.bottom = "215px";

                navBrand.style.left = "-35px";
                nav.style.flexDirection = "column";
                liItems.forEach((li) => {
                    li.style.display = "block";
                    li.style.fontSize = "20px";
                    li.style.background = "#555";
                    li.style.margin = "8px -58px";
                    li.children[0].style.color = "white";
                });

                navItems.style.flexDirection = "column";
            } else {
                currentItem.classList.remove("active");
                const liItems = document.querySelectorAll(".item");
                const hamburger = document.querySelector(".hamburger");
                const icon = document.querySelector(".straight-line");
                icon.style.top = "23.3em";

                liItems.forEach((li) => {
                    li.style.display = "none";
                    console.log(li);
                });

                hamburger.style.bottom = "90px";
                hamburger.style.left = "200px";
            }
        }
    </script>
    <script src="~/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js" asp-append-version="true"></script>
    <script src="~/lib/jquery-validation-unobtrusive/dist/jquery.validate.unobtrusive.min.js" asp-append-version="true"></script>
    <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    @await this.RenderSectionAsync("Scripts", required: false)
</body>
</html>
